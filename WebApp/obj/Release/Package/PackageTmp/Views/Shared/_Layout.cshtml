<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Taskesti-@ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/css?family=PT+Sans:400,400i,700,700i&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,400i,700,700i,900,900i&display=swap" rel="stylesheet">

    <!-- Stylesheets -->
    <link href="~/Taskesti/css/font-awesome-all.css" rel="stylesheet">
    <link href="~/Taskesti/css/flaticon.css" rel="stylesheet">
    <link href="~/Taskesti/css/owl.css" rel="stylesheet">
    <link href="~/Taskesti/css/bootstrap.css" rel="stylesheet">
    <link href="~/Taskesti/css/jquery.fancybox.min.css" rel="stylesheet">
    <link href="~/Taskesti/css/animate.css" rel="stylesheet">
    <link href="~/Taskesti/css/imagebg.css" rel="stylesheet">
    <link href="~/Taskesti/css/jquery-ui.css" rel="stylesheet">
    <link href="~/Taskesti/css/color.css" rel="stylesheet">
    <link href="~/Taskesti/css/style.css" rel="stylesheet">
    <link href="~/Taskesti/css/responsive.css" rel="stylesheet">
    
</head>
<body>

    <header class="main-header style-two">
        <!-- header-top -->
        <div class="header-top">
            <div class="auto-container clearfix">
                <div class="top-left pull-left">
                    <ul class="info-box clearfix">
                        <li><p><i class="fas fa-phone"></i> <strong>Tel No: </strong> <a href="tel:08508881414">0850 888 14 14</a></p></li>
                        <li><p><i class="fas fa-envelope"></i> <strong>E-posta:</strong> <a href="mailto:bilgi@taskestisu.com"> bilgi@taskestisu.com</a></p></li>
                       
                    </ul>
                </div>
                <div class="top-right pull-right">
                    <ul class="social-links clearfix">
                        <li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
                        <li><a href="#"><i class="fab fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fab fa-instagram"></i></a></li>
                       
                    </ul>
                </div>
            </div>
        </div><!-- header-top end -->
        <div class="outer-container">
            <div class="auto-container">
                <div class="header-upper clearfix">
                    <div class="upper-left pull-left clearfix">
                        <figure class="logo-box"><a href="index.html"> <img style="height:50px" src="~/Taskesti/images/resimler/logo.png" alt=""></a></figure>
                    </div>
                    <div class="upper-right pull-right clearfix">
                        <div class="menu-area pull-left">
                            <!--Mobile Navigation Toggler-->
                            <div class="mobile-nav-toggler">
                                <i class="icon-bar"></i>
                                <i class="icon-bar"></i>
                                <i class="icon-bar"></i>
                            </div>
                            <nav class="main-menu navbar-expand-md navbar-light">
                                <div class="collapse navbar-collapse show clearfix" id="navbarSupportedContent">
                                    <ul class="navigation clearfix">
                                        <li class="dropdown">
                                            <a href="@Url.Action("Index","Home")">ANASAYFA</a>

                                        </li>
                                        <li class="dropdown">
                                            <a href="@Url.Action("Kurumsal","Home")">KURUMSAL</a>

                                        </li>
                                        <li class="dropdown">
                                            <a href="@Url.Action("Urunler","Home")">ÜRÜNLERİMİZ</a>

                                        </li>
                                        <li class="dropdown">
                                            <a href="@Url.Action("SiparisVer","Home")">SİPARİŞ VER</a>

                                        </li>
                                        <li class="dropdown">
                                            <a href="@Url.Action("Belgeler","Home")">BELGELERİMİZ</a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="@Url.Action("Contact","Home")">İLETİŞİM</a>
                                        </li>
                                    </ul>
                                </div>
                            </nav>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!--sticky Header-->
        <div class="sticky-header">
            <div class="auto-container clearfix">
                <figure class="logo-box"><a href="index.html"> <img style="height:50px" src="~/Taskesti/images/resimler/logo.png" alt=""></a></figure>
                <div class="menu-area">
                    <nav class="main-menu clearfix">
                        <!--Keep This Empty / Menu will come through Javascript-->
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- main-header end -->
    <!-- Mobile Menu  -->
    <div class="mobile-menu">
        <div class="menu-backdrop"></div>
        <div class="close-btn"><i class="fas fa-times"></i></div>

        <nav class="menu-box">
            <div class="nav-logo"><a href="index.html"><img src="../../Taskesti/images/footer-logo.png" alt="" title=""></a></div>
            <div class="menu-outer"><!--Here Menu Will Come Automatically Via Javascript / Same Menu as in Header--></div>
            <div class="contact-info">
                <h4>Contact Info</h4>
                <ul>
                    <li>Chicago 12, Melborne City, USA</li>
                    <li><a href="tel:+8801682648101">+88 01682648101</a></li>
                    <li><a href="mailto:info@example.com">info@example.com</a></li>
                </ul>
            </div>
            <div class="social-links">
                <ul class="clearfix">
                    <li><a href="#"><span class="fab fa-twitter"></span></a></li>
                    <li><a href="#"><span class="fab fa-facebook-square"></span></a></li>
                    <li><a href="#"><span class="fab fa-pinterest-p"></span></a></li>
                    <li><a href="#"><span class="fab fa-instagram"></span></a></li>
                    <li><a href="#"><span class="fab fa-youtube"></span></a></li>
                </ul>
            </div>
        </nav>
    </div><!-- End Mobile Menu -->
    <!--Page Title-->
    <div>
        @RenderBody()
    </div>
    <footer class="main-footer">
    
        <div class="footer-upper">
            <div class="auto-container">
                <div class="widget-section wow fadeInUp" data-wow-delay="300ms" data-wow-duration="1500ms">
                    <div class="row clearfix">
                        <div class="col-md-6 col-sm-12">
                            <h2 style="color: white;">MERKEZ:</h2>
                            <p style="color: white"> <i class="fas fa-map-marker-alt text-white"></i> Tekstilkent Cad. Koza Plaza B Blok No:29 Esenler / İstanbul</p>
                            <p style="color: white"><i class="fas fa-phone"></i> <strong>Tel No: </strong> <a href="tel:08508881414">0850 888 14 14</a></p>
                            <p style="color: white"><i class="fas fa-phone"></i> <strong>Faks No:</strong>  <a href="tel:(212) 232 04 83">(212) 232 04 83</a></p>
                            <p style="color: white"><i class="fas fa-envelope"></i> <strong>E-posta:</strong> <a href="mailto:bilgi@taskestisu.com"> bilgi@taskestisu.com</a></p>
                            <p style="color: white"><i class="fas fa-envelope"></i> <strong>E-posta:</strong> <a href="mailto:export@taskestisu.com"> export@taskestisu.com</a></p>
                        </div>
                        <div class="col-md-6 col-sm-12">
                            <h2 style="color: white;">DOLUM TESİSİ:</h2>
                            <p style="color: white;"><i class="fas fa-map-marker-alt"></i> Merkez Mahallesi Karaçomak Küme Evleri No: 29 14810 Taşkesti-Bolu</p>
                            <p style="color: white;"><i class="fas fa-phone"></i> <strong>Tel No: </strong> <a href="tel:(0374)4266767">(0374) 426 67 67</a></p>
                            <p style="color: white;"><i class="fas fa-phone"></i> <strong>Faks No:</strong>  <a href="tel:(0374)4266505"> (0374) 426 65 05</a></p>
                            <p style="color: white;"><i class="fas fa-envelope"></i> <strong>E-posta:</strong> <a href="mailto:bilgi@taskestisu.com"> bilgi@taskestisu.com</a></p>
                        
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer-bottom">
            <div class="auto-container">
                <div class="copyright">@DateTime.Now.Year TAŞKESTİ</div>
            </div>
        </div>
    </footer>
    <!-- jequery plugins -->
    <script src="~/Taskesti/js/jquery.js"></script>
    <script src="~/Taskesti/js/popper.min.js"></script>
    <script src="~/Taskesti/js/bootstrap.min.js"></script>
    <script src="~/Taskesti/js/owl.js"></script>
    <script src="~/Taskesti/js/wow.js"></script>
    <script src="~/Taskesti/js/validation.js"></script>
    <script src="~/Taskesti/js/jquery.fancybox.js"></script>
    <script src="~/Taskesti/js/scrollbar.js"></script>
    <script src="~/Taskesti/js/jquery-ui.js"></script>
    <script src="~/Taskesti/js/appear.js"></script>

    <!-- map script -->
    @*<script src="http://maps.google.com/maps/api/js?key=AIzaSyATY4Rxc8jNvDpsK8ZetC7JyN4PFVYGCGM"></script>*@
    <script src="~/Taskesti/js/gmaps.js"></script>
    <script src="~/Taskesti/js/map-helper.js"></script>

    <!-- main-js -->
    <script src="~/Taskesti/js/script.js"></script>
    @RenderSection("scripts",false)

</body>
</html>
